{
  "name": "pokevue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "convert-images": "sharp -i src/assets/images/pokemons/*.png -o src/assets/images/pokemons/webp --format=webp",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.9.0",
    "pinia": "^3.0.3",
    "sass": "^1.89.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.5.3",
    "sharp": "^0.34.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.8"
  }
}
